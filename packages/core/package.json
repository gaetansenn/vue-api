{
  "name": "@vue-api/core",
  "version": "1.0.1",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./node": {
      "import": "./dist/node.mjs",
      "types": "./dist/node.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "vue-api": "./dist/runtime/bin/cli.mjs"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.23.4",
    "@vue/test-utils": "^2.0.2",
    "c8": "^7.12.0",
    "citty": "^0.1.2",
    "jsdom": "^20.0.0",
    "typescript": "latest",
    "unbuild": "^2.0.0",
    "vitest": "^0.28.5",
    "vue": "^3.5.5",
    "fast-glob": "^3.3.2"
  },
  "scripts": {
    "prepare": "unbuild",
    "dev": "unbuild",
    "build": "unbuild",
    "changelog": "standard-version"
  },
  "dependencies": {
    "@vueuse/core": "^9.10.0"
  },
  "standard-version": {
    "skip": {
      "bump": true,
      "commit": true,
      "tag": true
    },
    "tag-prefix": "@vunix/core@"
  },
  "peerDependencies": {
    "ofetch": "~1.1.1"
  }
